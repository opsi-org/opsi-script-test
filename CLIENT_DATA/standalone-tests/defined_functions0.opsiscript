

[actions]
setloglevel=8
ScriptErrorMessages = false


DefVar $mystr$
DefVar $str1$
DefVar $str2$
DefStringlist $list1$
DefStringlist $list2$
set $str1$ = 'ha'
set $str2$ = 'hi'
set $mystr$ = $str1$ + $str2$


DefFunc myFunc(val $str1$ : string, $str2$ : string) : string
	set $result$ = $str1$ + $str2$
endfunc


set $mystr$ = myFunc("he",timeStampAsFloatStr)


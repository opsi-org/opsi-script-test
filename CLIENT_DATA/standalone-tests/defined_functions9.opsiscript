

[actions]
setloglevel=8
ScriptErrorMessages = false

importFunc "functionlib.opsiscript"

DefVar $mystr$
DefVar $str1$
DefVar $str2$
DefStringlist $list1$
DefStringlist $list2$
set $str1$ = 'ha'
set $str2$ = 'hi'
set $mystr$ = $str1$ + $str2$

if GetOS = 'Linux'
	set $list1$ = myFunc8('pwd')
else
	set $list1$ = myFunc8('echo %cd%')
endif

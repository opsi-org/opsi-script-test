	DefFunc compareValueWithExpected(): void
		if ($ConstTest$ = $CompValue$)
			comment "PASSED (Value: " + $ConstTest$ + ", Expected: " + $CompValue$ + ")"
		else
			set $TestResult$ = "not o.k."
			LogWarning "FAILED (Value: " + $ConstTest$ + ", Expected: " + $CompValue$ + ")"
		endif

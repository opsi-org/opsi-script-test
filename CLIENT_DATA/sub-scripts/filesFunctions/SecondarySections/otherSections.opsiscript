[sub_check_dir_exist_inprof]
if directoryexists("%prof%\opsi-script-test")
	comment "passed"
else
	set $TestResult$ = "not o.k."
	LogWarning "failed"
endif

[sub_check_dir_not_exist_inprof]
if not(directoryexists("%prof%\opsi-script-test"))
	comment "passed"
else
	set $TestResult$ = "not o.k."
	LogWarning "failed"
endif

[DosInAnIcon_prepare_copy_u]
c:
cd tmp
mkdir "$HomeTestFiles$"
mkdir "$HomeTestFiles$\files"
cd "$HomeTestFiles$"
echo Hello World > dummy.txt
copy dummy.txt "files\dummy.txt"
cd "$HomeTestFiles$\files"
echo Hello World Files> dummy2.txt
cd ..
ping 127.0.0.1 -n 60
echo Hello World > dummy2.txt
cd "%opsiTmpDir%"
exit

